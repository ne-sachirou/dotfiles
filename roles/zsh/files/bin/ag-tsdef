#!/usr/bin/env bb
;; -*- mode: clojure; -*-
(require '[babashka.process :refer [process]])


;; ag -G '(?<!\.test)\.ts(x?)$' "(class|const|function|interface|let|type|var|(^ *)) $1(?=\W)"
(-> ["ag"
     "-G" "(?<!\\.test)\\.ts(x?)$"
     "--color"
     (str "(class|const|function|interface|let|type|var|(^ *)) " (first *command-line-args*) "(?=\\W)")]
    (process {:in :inherit})
    :out
    slurp
    println)
