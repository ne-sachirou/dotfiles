[core]
	longpath = true
	quotepath = false
	sparseCheckout = false
[color]
	ui = true
[user]
	name = {{ user_name | default("ne_Sachirou") }}
	email = {{ user_email | default("utakata.c4se@gmail.com") }}
[http]
[https]
[url "https://"]
[http]
[https]
[url "https://"]
[alias]
	br = branch
	ci = commit
	co = checkout
	graph = log --decorate=full --graph --oneline
	pull-p = !git pull -p --all && git gone && etags-of
	push-f = push --force-if-includes --force-with-lease
	st = status -sb
	sw = switch
[pull]
	rebase = false
[push]
	default = simple
[init]
	defaultBranch = main
